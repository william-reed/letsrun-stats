{
  "manifest_version": 2,
  "name": "LetsRun Stats",
  "version": "0.0.1",
  "author": "William Reed",
  "description": "Bringing you stats about all the anonymous users of LetsRun",
  "permissions": [
    "*://www.letsrun.com/forum/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.letsrun.com/forum/flat_read.php?thread=*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "stats.js"
      ]
    }
  ]
}